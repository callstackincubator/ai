{
  "name": "@react-native-ai/apple",
  "version": "0.9.0",
  "description": "Apple LLM provider for Vercel AI SDK",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "license": "MIT",
  "homepage": "https://github.com/callstackincubator/ai#readme",
  "author": "Mike Grabowski <grabbou@gmail.com>",
  "scripts": {
    "clean": "del-cli lib",
    "typecheck": "tsc --noEmit",
    "prepare": "bob build"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@ai-sdk/provider-utils": "^3.0.10",
    "zod": "^4.0.0"
  },
  "peerDependencies": {
    "react-native": "^0.79.5"
  },
  "devDependencies": {
    "react-native": "0.81.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "NativeAppleLLM",
    "type": "modules",
    "jsSrcsDir": "src",
    "ios": {
      "modulesProvider": {
        "NativeAppleLLM": "AppleLLM",
        "NativeAppleEmbeddings": "AppleEmbeddings",
        "NativeAppleTranscription": "AppleTranscription",
        "NativeAppleSpeech": "AppleSpeech",
        "NativeAppleUtils": "AppleUtils"
      }
    }
  },
  "keywords": [
    "react-native",
    "apple",
    "llm",
    "ai",
    "sdk",
    "vercel"
  ]
}
