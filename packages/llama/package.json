{
  "name": "@react-native-ai/llama",
  "version": "0.10.0",
  "description": "llama.rn provider for Vercel AI SDK",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "license": "MIT",
  "homepage": "https://github.com/callstackincubator/ai#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callstackincubator/ai.git"
  },
  "bugs": {
    "url": "https://github.com/callstackincubator/ai/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Szymon Rybczak <szymon.rybczak@gmail.com> (https://github.com/szymonrybczak)",
  "contributors": [
    "Mike Grabowski <grabbou@gmail.com> (https://github.com/grabbou)"
  ],
  "scripts": {
    "clean": "del-cli lib",
    "typecheck": "tsc --noEmit",
    "prepare": "bob build"
  },
  "dependencies": {
    "@ai-sdk/provider": "^3.0.5",
    "@ai-sdk/provider-utils": "^4.0.1",
    "react-native-blob-util": "^0.24.5",
    "zod": "^4.0.0"
  },
  "peerDependencies": {
    "react-native": ">=0.76.0",
    "llama.rn": "^0.10.1"
  },
  "devDependencies": {
    "react-native": "0.81.4",
    "llama.rn": "^0.10.1"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "keywords": [
    "react-native",
    "llama",
    "llm",
    "ai",
    "sdk",
    "vercel",
    "gguf",
    "on-device"
  ]
}
